{
  "name": "github-asana-issue-sync-cloudflare",
  "version": "1.0.0",
  "description": "Cloudflare Worker to sync GitHub issues to Asana tasks via webhooks",
  "type": "module",
  "main": "src/index.js",
  "scripts": {
    "dev": "wrangler dev --local --port 8787",
    "dev:remote": "wrangler dev",
    "deploy": "wrangler deploy",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:webhook": "node dev/test-webhook.js",
    "test:webhook:opened": "node dev/test-webhook.js issueOpened",
    "test:webhook:edited": "node dev/test-webhook.js issueEdited",
    "test:webhook:closed": "node dev/test-webhook.js issueClosed",
    "test:webhook:reopened": "node dev/test-webhook.js issueReopened",
    "test:webhook:comment": "node dev/test-webhook.js commentCreated",
    "logs": "wrangler tail"
  },
  "dependencies": {
    "asana": "^3.0.2"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241106.0",
    "wrangler": "^3.78.12",
    "vitest": "^2.1.3"
  }
}